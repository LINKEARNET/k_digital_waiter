<link rel="stylesheet" href="/css/Profile.css">

<body>
    <section class="seccion-perfil-usuario">
        <div class="perfil-usuario-header">
            <div class="perfil-usuario-portada">
                <div class="perfil-usuario-avatar">
                    {{#if user.profile_image}}
                    <img src="/{{user.profile_image}}" loading="lazy" alt="User Profile">
                    {{else}}
                    <img src="/not-found/not-found-image.png" alt="..." alt="User Profile">
                    {{/if}}

                    <button type="button" class="boton-avatar">
                        <a class="far fa-image" href="#modal"></a>
                    </button>
                </div>

                <form id="modal" class="modal" action="/users/image-profile" method="POST"
                    encType="multipart/form-data">
                    <div class="modal_container">
                        <div class="drag-area">
                            <h2 class="modal_title">Editar foto de
                                <span class="modal_title modal_title--bold">{{user.name}} </span>
                            </h2>
                            <input type="file" name="sampleFile" accept="image/*" />
                            <input type="submit">
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <div class="perfil-usuario-body">
            <div class="perfil-usuario-bio">
                <h3 class="titulo" style="color: rgb(93, 190, 217);">{{user.name}} </h3>
                <p class="texto">{{user.description}}</p>
            </div>
            <div class="perfil-usuario-footer">
                <ul class="lista-datos">
                    
                    <li><i class="icono fas fa-map-signs"></i> Direccion de usuario: {{user.address}}</li>
                    <li><i class="icono fas fa-phone-alt"></i> Telefono: {{user.phone}}</li>
                    <li><i class="icono fas fa-briefcase"></i> Correo: {{user.email}}</li>
                    
                </ul>
                <ul class="lista-datos">
                    
                    <li><i class="icono fas fa-calendar-alt"></i> Rol: administrador </li>
                    <li><i class="icono fas fa-user-check"></i> Registro: NÂ° {{user.id}}</li>
                    
                </ul>
            </div>
            
        </div>
    </section>


